	function max(style, data) {
		var max = 0, id;

		$(style).each(function() {
			id = $(this).data(data); 
			max = (max < id) ? id:max; 
		});

		return max;
	}

	function buttonStatus(status) {
		if (status != false) {
			$('.container-fluid button').removeAttr('disabled');
		} else {
